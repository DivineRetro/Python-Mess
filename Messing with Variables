age = 23
age = 25
print(age)

if age > 21:
    print("You can drink alcoholic beverages.")

firstName = "Joe"
print(firstName)

lastName = 'Erickson'
print(lastName)

print(firstName, lastName)

age = 21
age = float(age)
print(age)

isStudent = False
print(isStudent)

isStudent = 'hello'
print(isStudent)

print("My variables are", str(age), firstName, lastName, isStudent)

print("My name is", firstName)
